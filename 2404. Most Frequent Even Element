/**
 * @param {number[]} nums
 * @return {number}
 */
var mostFrequentEven = function (nums) {
    let countMap = {};

    for (let i = 0; i < nums.length; i++) {
        if (nums[i] % 2 === 0) {
            if (!countMap[nums[i]]) {
                countMap[nums[i]] = 1;
            } else {
                countMap[nums[i]] += 1;
            }
        }
    }

    let maxValue = -Infinity;
    for (let values in countMap) {
        if (countMap[values] > maxValue) {
            maxValue = countMap[values]
        }
    }
    let arr = []
    for (let keys in countMap) {
        if (countMap[keys] === maxValue) {
            arr.push(Number(keys))
        }
    }

    if (arr.length === 0) {
        return -1
    }

    let minValue = Infinity
    for (let i = 0; i <= arr.length; i++) {
        if (arr[i] < minValue) {
            minValue = arr[i]
        }
    }
    return minValue
};

/**
 * @param {number[]} nums1
 * @param {number} m
 * @param {number[]} nums2
 * @param {number} n
 * @return {void}
 */
var merge = function (nums1, m, nums2, n) {
    let merged = []
    for (let i = 0; i < m; i++) {
        merged.push(nums1[i])
    }
    for (let i = 0; i < n; i++) {
        merged.push(nums2[i])
    }
    let sorted = merged.sort((a, b) => a - b)
    
    for (let i = 0; i < m + n; i++) {
        nums1[i] = merged[i];  
    }
};
